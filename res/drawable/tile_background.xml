<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:id="@+drawable/tile_background_item"
        android:bottom="5dip"
        android:left="5dip"
        android:right="5dip"
        android:top="5dip">
        <shape android:shape="rectangle" >
            <solid android:color="@color/tile_background" />

            <stroke
                android:width="1px"
                android:color="@color/tile_stroke" />

            <padding
                android:bottom="10dip"
                android:left="10dip"
                android:right="10dip"
                android:top="10dip" />
        </shape>
    </item>

</layer-list>